{
  "name": "cpsit/t3events-template",
  "type": "typo3-cms-extension",
  "description": "Event Templates is an extension for the TYPO3 CMS. It extends t3events by templates.",
  "keywords": [
    "TYPO3",
    "extension",
    "events",
    "template"
  ],
  "license": [
    "GPL-2.0+"
  ],
  "authors": [
    {
      "name": "Dirk Wenzel",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "typo3/cms-core": ">=8.7.0 || >=9.5.0",
    "dwenzel/t3events": ">=1.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6 || ^6.0",
    "nimut/testing-framework": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "CPSIT\\T3eventsTemplate\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CPSIT\\T3eventsTemplate\\Tests\\": "Tests"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "preferred-install": {
      "*": "dist"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "mkdir -p .Build/log/coverage/",
      "[ -L .Build/Web/typo3conf/ext/t3events_template ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/t3events_template"
    ]
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
